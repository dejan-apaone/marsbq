<section id="menu" class="menu-container">
	<div class="wrapp">
		<h2>Menu</h2>
		<p>Plaese vote for your favorite items</p>
		<ul>
			{{#each menu}}
			<li data-id="{{id}}">
				<span class="menu-item">{{title}}</span>
				<span class="menu-count">{{count}}</span>
			</li>
			{{/each}}
		</ul>
	</div>
</section>
